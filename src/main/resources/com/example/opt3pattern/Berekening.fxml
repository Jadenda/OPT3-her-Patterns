<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.opt3pattern.BerekeningController">
   <children>
      <Label alignment="TOP_CENTER" layoutX="53.0" layoutY="32.0" prefHeight="22.0" prefWidth="483.0" text="Kies hier de soort berekening">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </Label>
      <Button fx:id="Loguit2" layoutX="516.0" layoutY="354.0" mnemonicParsing="false" onMouseClicked="#onLoguit2Click" prefHeight="32.0" prefWidth="70.0" text="Loguit" textFill="#e10909">
         <font>
            <Font name="Arial" size="14.0" />
         </font></Button>
      <Button fx:id="Brutowinst" layoutX="194.0" layoutY="68.0" mnemonicParsing="false" onMouseClicked="#onBrutowinstClick" text="Hoeveel bedraagt mijn brutowinst">
